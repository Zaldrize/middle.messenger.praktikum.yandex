{
  "env": {
    "node": true,
    "browser": true
    },
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
      "eol-last": "error",
      "max-len": [2, 100],
      "@typescript-eslint/no-unused-vars": 2
      },
      "overrides": [
        {
            "files": ["*.ts"],
            "rules": {
                "no-undef": "off"
            }
        }
    ]
  }
